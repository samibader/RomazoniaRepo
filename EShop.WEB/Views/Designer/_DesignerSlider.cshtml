@model List<EShop.BLL.DTO.DesignerDTO>
@{
    Layout = null;
}
<style>

</style>
<div class="slider-items-products">
    <div id="our-clients-slider" class="product-flexslider hidden-buttons">
        <div class="slider-items slider-width-col6">
            @foreach (var designerDto in Model)
            {
                <!-- Item -->
                <div class="item" style="border: 1px solid #d7d7d7; padding: 5px 0px;">
                    <a href="@Url.Action("Browse","Designer",new {designerName=designerDto.DesignerName})"><img src="@(designerDto.ImageUrl).ashx?height=90" alt="Image"></a>
                </div>
                <!-- End Item -->
            }
        </div>
    </div>
</div>
